; this program reads two integers and prints the smaller one
; this program functions as a test case for control flows: label, branch, goto

.raw
.class
.function
defun main 0 NULL
stores 0 30 "print the smaller from a and b"
load 0
call println
stores 0 4 "a = "
load 0
call print
call readint      ; reads one integer from command line
stores 0 4 "b = "
stores 1 1 "a"
stores 2 1 "b"
load 0
call print
call readint
lt
branch 1
load 1
goto 2
label 1
load 2
label 2
call println
endef
